    <script>
        $(function () {
            /*公告栏信息滚动*/
            var i=0;
            var num= $('.notice').find('li').length;
            $('.notice').find('ul').clone().appendTo('.notice');
            function slide() {
                $('.notice').find('ul').eq(0).animate({'margin-top': '-=20px'});
                i++;
                if(i%num==0){
                    // var mar_top=$('.notuce_list').find('li').eq(i).css('margin-top');
                    $('.notice').find('ul').eq(0).remove().clone().appendTo('.notice').attr('style','');
                }

            }
            setInterval(slide,2000);

            /*评论区选项卡*/
            $('.connect_content .ccUl').find('li').click(function () {
                var targetLiId=$(this).attr('id');
                $(this).addClass('hover').siblings('li').removeClass('hover');
                $('#con_'+targetLiId).show().siblings('.con').hide();
            });
            /*最后一个评论动态改变样式*/
            $('.cmUl li').last().children('dl').first().css('padding-bottom','10px');
            $('.cmUl li').last().children('dl').last().css({
                'border-top':'1px dashed #cfd6dc',
                'margin-left':'60px',
                'padding-top':'10px'
            });
            var maxLength=200;
            $('.connect').find('#text').keyup(function () {
                if ($(this).val().length>maxLength){
                    alert('字数超过');
                }else {
                    $('.begin_con').html($(this).val().length);
                }
            });
        });
    </script>
